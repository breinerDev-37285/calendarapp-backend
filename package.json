    "@types/node-fetch": "^2.5.8",
    "@types/redis": "^2.8.28",
    "dotenv": "^8.2.0",
    "link-module-alias": "^1.2.0",
    "nodemon": "^2.0.7",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.1.5"
  },
  "dependencies": {
    "@types/mongoose-unique-validator": "^1.0.4",
    "audit": "0.0.6",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-validator": "^6.9.2",
