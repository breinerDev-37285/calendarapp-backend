{
  "name": "backend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "init": "tsc --init",
    "build": "tsc --build && npm run alias",
    "alias": "link-module-alias",
    "dev": "nodemon --ext ts,js,json --exec 'ts-node -r tsconfig-paths/register src/index.ts'",
    "start": "node ./dist/index.js",
    "docker": "npm run cleanDocker && tsc --build && npm run image && npm run container && rm -rf ./dist",
    "image": "docker build --tag calendarapp .",
    "container": "docker run --name calendar -p 3000:3000 -d calendarapp",
    "cleanDocker": "docker rmi calendarapp -f && docker rm -fv calendar"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "_moduleAliases": {
    "@config": "dist/app/config",
    "@app": "dist/app/app.js",
    "@server": "dist/app/server",
    "@routes": "dist/app/routes",
    "@services": "dist/app/services",
    "@middlewares": "dist/app/middlewares",
    "@typesSrc": "dist/app/types",
    "@database": "dist/app/database",
    "@interfaces": "dist/app/interfaces",
    "@helpers": "dist/app/helpers"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.9",
    "@types/express": "^4.17.11",
